gin-bottom: 10px;\r\n}\r\n\r\n#gameBoard {\r\n  display: grid;\r\n  grid-template-areas:\r\n    \"one two three\"\r\n    \"four five six\"\r\n    \"seven eight nine\";\r\n  border-radius: 5px;\r\n  box-shadow: -3px 8px 10px rgba(0, 0, 0, 0.26);\r\n  background: url(\"./assets/gameBoard.png\"), #fff;\r\n  background-repeat: no-repeat;\r\n  background-size: contain;\r\n  border: 3px solid #000;\r\n  padding: 0 5px 6px 0;\r\n  height: 62vh;\r\n  width: 70vh;\r\n}\r\n#gameBoard > div {\r\n  cursor: pointer;\r\n  transition: background-color 1s;\r\n  text-align: center;\r\n  position: relative;\r\n}\r\n\r\n#gameBoard > div > div {\r\n  position: absolute;\r\n  top: 15%;\r\n  left: 50%;\r\n  z-index: 2;\r\n  transform: translateX(-45%);\r\n  font-size: 4rem;\r\n}\r\n\r\n#gameBoard > div:hover {\r\n  background-color: rgba(102, 102, 102, 0.233);\r\n}\r\n\r\n#one {\r\n  grid-area: one;\r\n}\r\n#two {\r\n  grid-area: two;\r\n}\r\n#three {\r\n  grid-area: three;\r\n}\r\n#four {\r\n  grid-area: four;\r\n}\r\n#five {\r\n  grid-area: five;\r\n}\r\n#six {\r\n  grid-area: six;\r\n}\r\n#seven {\r\n  grid-area: seven;\r\n}\r\n#eight {\r\n  grid-area: eight;\r\n}\r\n#nine {\r\n  grid-area: nine;\r\n}\r\n\r\n/* Modal */\r\n\r\n#modal {\r\n  background-color: #fff;\r\n  width: 300px;\r\n  border-radius: 5px;\r\n  box-shadow: -3px 8px 10px rgba(0, 0, 0, 0.26);\r\n  padding: 10px;\r\n}\r\n\r\n#modal h1 {\r\n  margin-bottom: 20px;\r\n}\r\n#modal h3 {\r\n  margin-bottom: 10px;\r\n}\r\n\r\n#modal button {\r\n  background-color: limegreen;\r\n  border: none;\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n  color: #fff;\r\n  cursor: pointer;\r\n}\r\n\r\n#modal button span {\r\n  padding: 20px;\r\n  font-size: 1.5rem;\r\n}\r\n\r\n#backdrop {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  position: absolute;\r\n  height: 100%;\r\n  width: 100%;\r\n  text-align: center;\r\n  z-index: 5;\r\n  background: rgba(0, 0, 0, 0.5);\r\n}\r\n\r\n@media screen and (max-wid